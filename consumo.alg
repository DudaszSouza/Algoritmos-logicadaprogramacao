Algoritmo "consumo"

Var

   distancia, combustivel, consumoMedio : real

Inicio

   escreva("Qual foi a distancia pecorrida: ")
   leia(distancia)
   escreva("Qual a quantidade de combustivel gasto: ")
   leia(combustivel)

   consumoMedio <- distancia / combustivel

   escreval("O Consumo medio é de ", consumoMedio:8:3)

Fimalgoritmo